<!DOCTYPE html>
<html>
<head>
<style>
table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
}

td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
}

h2, p {
    text-align: center;
}
</style>
</head>
<body>

<h2>All Students Subjects Data</h2>
<table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Score</th>
      </tr>
    </thead>
    <tbody>
      <% studentSubjectData.forEach(studentSubject => { %>
        <tr>
          <td><%= studentSubject.Student.firstName %>
            <%= studentSubject.Student.lastName %></td>
          <% if(studentSubject.score !== null){ %>
            <td><%= studentSubject.score %></td>
          <% } else { %>
            <td><a href="/subjects/<%=studentSubject.StudentId%>/give-score/<%=studentSubject.SubjectId%>">Give Score</a></td>
          <% } %>
        </tr>
      <% })%>
    </tbody>
  </table>
</body>
</html>

